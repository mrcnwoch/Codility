   "source": [
    "def count_div(A, B, K):\n",
    "    div_cnt = 0\n",
    "    \n",
    "    for i in xrange(A, B + 1):\n",
    "        if i % K == 0:\n",
    "            div_cnt += 1\n",
    "    \n",
    "    return div_cnt"
   ]
