   "source": [
    "def solution(A):\n",
    "    A.sort()\n",
    "    B = []\n",
    "    \n",
    "    for i in xrange(len(A)):\n",
    "        if i == 0:\n",
    "            B.append(A[i])\n",
    "        else:\n",
    "            if A[i] != A[i - 1]:\n",
    "                B.append(A[i])\n",
    "    \n",
    "    return len(B)"
   ]
  },
